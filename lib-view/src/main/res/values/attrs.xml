<?xml version="1.0" encoding="utf-8" ?>
<resources>
    <declare-styleable name="BaseGridView">
        <attr format="boolean" name="focusOutFront"/>
        <attr format="boolean" name="focusOutEnd"/>
        <attr name="android:gravity"/>
    </declare-styleable>

    <declare-styleable name="HorizontalGridView">
        <attr name="numberOfRows" format="integer" />
    </declare-styleable>

    <declare-styleable name="VerticalGridView">
        <attr name="numberOfColumns" format="integer" />
    </declare-styleable>
</resources>